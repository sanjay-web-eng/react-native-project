{
	"expo": {
		"name": "Test App",
		"slug": "Test App",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/play_store_512.png",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,

		"ios": {
			"supportsTablet": true
		},
		"splash": {
			"image": "./assets/images/play_store_512.png", 
			"resizeMode": "contain",       
			"backgroundColor": "#6C63FF",
			"imageWidth": 50  
		  },

		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/play_store_512.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/play_store_512.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/play_store_512.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
